import java.util.Map;

public class ThreadMan {
    public static void main(String[] str){
      Map<String, String> a=  System.getenv();
        for(String s : a.keySet()){
            System.out.printf(s);
            System.out.printf(": ");
            System.out.printf(System.getenv().get(s));
            System.out.printf("%n");
        }
    }
}

